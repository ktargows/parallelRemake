# Generated by parallelRemake::write_makefile() on 2017-02-12 08:53:59 

.PHONY: clean  all plot.pdf mtcars random 

INIT := $(shell Rscript -e 'parallelRemake:::init_timestamps("remake.yml")')

all: .data/all

.data/all: .data/plot.pdf 

plot.pdf: .data/plot.pdf

.data/plot.pdf: .data/mtcars .data/random 
	Rscript -e 'if (!remake::is_current("plot.pdf")) remake::make("plot.pdf", remake_file = "remake.yml"); invisible(file.copy(".timestamp", ".data/plot.pdf", copy.date = FALSE, overwrite = TRUE))'

mtcars: .data/mtcars

.data/mtcars:  
	Rscript -e 'if (!remake::is_current("mtcars")) remake::make("mtcars", remake_file = "remake.yml"); invisible(file.copy(".timestamp", ".data/mtcars", copy.date = FALSE, overwrite = TRUE))'

random: .data/random

.data/random:  
	Rscript -e 'if (!remake::is_current("random")) remake::make("random", remake_file = "remake.yml"); invisible(file.copy(".timestamp", ".data/random", copy.date = FALSE, overwrite = TRUE))'

clean:
	Rscript -e 'remake::make("clean", remake_file = "remake.yml")'
	rm -rf .data .remake .timestamp
