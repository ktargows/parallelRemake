# Generated by parallelRemake::write_makefile() on 2017-02-11 20:47:26 

.PHONY: all plot.pdf mtcars random clean

all: plot.pdf 

plot.pdf: mtcars random 
	Rscript -e 'if (!remake::is_current("plot.pdf")) remake::make("plot.pdf", remake_file = "remake.yml")'

mtcars:  
	Rscript -e 'if (!remake::is_current("mtcars")) remake::make("mtcars", remake_file = "remake.yml")'

random:  
	Rscript -e 'if (!remake::is_current("random")) remake::make("random", remake_file = "remake.yml")'

clean:
	Rscript -e 'remake::make("clean", remake_file = "remake.yml")'
	rm -rf .remake
